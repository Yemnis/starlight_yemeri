{
  "name": "starlight-video-analysis-api",
  "version": "1.0.0",
  "description": "Video ingestion and retrieval system with multimodal AI analysis",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:coverage": "jest --coverage",
    "setup:gcp": "tsx scripts/setup-gcp.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": ["video", "ai", "gemini", "whisper", "vertex-ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vertexai": "^1.1.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "fluent-ffmpeg": "^2.1.2",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
